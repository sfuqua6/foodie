# Oracle Cloud Production Environment Variables
# Copy this to .env and update with your actual values

# Database Configuration (Oracle Autonomous Database)
DATABASE_URL=postgresql://username:password@autonomous-db-host:1522/ratemyrest

# Redis Configuration (OCI Cache with Redis)
REDIS_URL=redis://redis-cache-host:6379

# Security
SECRET_KEY=your-strong-secret-key-here-min-32-chars
ENVIRONMENT=production

# Google Places API
GOOGLE_PLACES_API_KEY=your-google-places-api-key

# Frontend Configuration
REACT_APP_API_URL=https://your-domain.com
REACT_APP_ENVIRONMENT=production

# Logging
LOG_LEVEL=info